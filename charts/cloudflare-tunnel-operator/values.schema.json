{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "cloudflareToken": {
      "type": "object",
      "properties": {
        "existingSecret": {
          "type": "string"
        },
        "cloudflareAPIToken": {
          "type": "string"
        },
        "cloudflareAccountID": {
          "type": "string",
          "minLength": 1
        },
        "cloudflareZoneID": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "cloudflareAccountID",
        "cloudflareZoneID"
      ],
      "oneOf": [
        {
          "required": [
            "cloudflareAPIToken"
          ]
        },
        {
          "required": [
            "existingSecret"
          ]
        }
      ]
    }
  },
  "title": "Values",
  "type": "object"
}
